<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false"
    tools:context="com.example.admin.rxjavaexamples.MainActivity">


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >


    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="simpleActivity"
        android:text="Simple"
         />
    <Button
        android:id="@+id/publish"
        android:onClick="publishActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="publish"
        />

    <Button
        android:id="@+id/replayActivity"
        android:onClick="replayActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Replay"
        />
    <Button
        android:id="@+id/behaviourActivity"
        android:onClick="behaviourActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Behaviour"
        />
    <Button
        android:id="@+id/asyncActivity"
        android:onClick="asyncActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Aync"
        />

    <Button
        android:id="@+id/single"
        android:onClick="singleActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Single"
        />

    <Button
        android:id="@+id/defer"
        android:onClick="deferActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Defer"
        />
    <Button
        android:id="@+id/buffer"
        android:onClick="bufferActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="buffer"
        />
    <Button
        android:id="@+id/concat"
        android:onClick="cancatActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cancat"
        />
    <Button
        android:id="@+id/debounce"
        android:onClick="debounceActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Debounce"
        />
    <Button
        android:id="@+id/delay"
        android:onClick="delayActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Delay"
        />
    <Button
        android:id="@+id/disposable"
        android:onClick="disposableActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Disposable"
        />
    <Button
        android:id="@+id/distinct"
        android:onClick="distinctActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Distinct"
        />
    <Button
        android:id="@+id/filter"
        android:onClick="filterActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Filter"
        />
    <Button
        android:id="@+id/flowable"
        android:onClick="flowableActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Flowable"
        />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!"
            />

    </RelativeLayout>

</LinearLayout>
</ScrollView>
